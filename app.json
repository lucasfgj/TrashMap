{
  "expo": {
    "name": "TrashMap",
    "slug": "TrashMap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/images/logoTrashMap.png",
    "scheme": "trashmap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCBs_5rLELLShtl5MR3lnIqova7IBWDGZg"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logoTrashMap.png",
        "backgroundImage": "./assets/images/backgroundTrashMap.png",
        "monochromeImage": "./assets/images/backgroundTrashMap.png"
      },
      "softwareKeyboardLayoutMode": "resize",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCBs_5rLELLShtl5MR3lnIqova7IBWDGZg"
        }
      },
      "package": "com.anonymous.TrashMap"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoTrashMap.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
